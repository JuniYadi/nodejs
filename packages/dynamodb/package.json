{
  "name": "@juniyadi/dynamodb",
  "version": "0.2.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.mjs",
        "types": "./dist/index.d.mts"
      },
      "require": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@aws-sdk/lib-dynamodb": "^3.388.0",
    "@juniyadi/tsconfig": "workspace:*",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.9",
    "eslint": "^8.46.0",
    "eslint-config-juniyadi": "workspace:*",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "tsup": "^5.12.9",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "^3.388.0",
    "@aws-sdk/lib-dynamodb": "^3.388.0"
  }
